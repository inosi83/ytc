package org.comstudy21.ex01;

import java.util.Scanner;

public class Ex02 {
	static Scanner scan = new Scanner(System.in);
	static String getName(){
		System.out.print("ΌΊΈν: ");
		String n = scan.next();
		
		return n;
	}
	public static void main(String[] args) {
		
		String name = getName();
		
		System.out.println("Name = "+ name);
	}
}
