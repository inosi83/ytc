package org.comstudy21.ex01;
//Point 클래스 선언.
class Point {
	int x;
	int y;
}

public class Ex03 {
	public static void main(String[] args) {
		//Point 클래스를 이용해서 객체 생성.
		Point pt01 = new Point();
		
		pt01.x = 30;
		pt01.y = 40;
		
		System.out.println("pt01.x = "+pt01.x);
		System.out.println("pt01.y = "+pt01.x);
	}//main
}//class
