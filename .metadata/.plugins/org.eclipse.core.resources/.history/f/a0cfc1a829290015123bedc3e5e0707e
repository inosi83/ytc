package org.comstudy21.ex01;

public class Ex07 {

	public static void main(String[] args) {
		//로또번호 생성기 만들기
		int[] lotto = new int[6];
		int cnt = 0;
		
		while(cnt<6){
			int number = (int)(1 + Math.random()*45);
			lotto[cnt] = number;
			cnt++;
		}
		for(int i=0; i<lotto.length; i++){
			System.out.print(lotto[i] + " ");
		}
		
	}
}
