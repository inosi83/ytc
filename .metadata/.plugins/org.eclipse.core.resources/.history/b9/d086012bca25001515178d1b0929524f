import java.util.Scanner;

public class Ex05 {

	
	public static void main(String[] args){
		
		Scanner scan = new Scanner(System.in);
		int q = 0;
		
		System.out.print("ют╥б: ");
		q = scan.nextInt();
		
		if((q%2 == 0)&&(q<5)){
			System.out.print("ERROR!");
		}else{
			for(int a=0; a<q; a++){
				System.out.print("*");
			}//for1
			System.out.println();
			for(int b=0; b<q; b++){
				System.out.println("*");
				for(int c=0; c=q; c++){
					
				}
			}//for2
		}//else
	}//main
}
