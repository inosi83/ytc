package org.comstudy21.ex01;

public class Ex04 {

	public static void main(String[] args) {
		
		int[] arr = new int[]{60, 90, 80, 70, 100};
		int max = 0, min = 0;
		int size = arr.length;
		//arr 배열에서 가장 큰 값은 max에 가장 작은 값은 min대입
		//for 문을 이용해서 구현하라.
		for(int i=0; i<size; i++){
			max = arr[i];
			if(arr[i] < max){
				System.out.print(max);
			}
		}
	}
}
