package org.comstudy21.ex01;

public class Ex06 {

	public static void main(String[] args) {
		
		int[] arr = {1,2,3,4,5};
		int[] arr2 = {6,7,8,9};
		//arr배열에 arr2배열을 연결 하라.
		
		arr = new int[arr.length+arr2.length];
		
		int size = arr.length+arr2.length;

		for(int i=0; i<size; i++){
			
			System.out.print(arr[i]);
		}
		
	}
}
